    },
    "homepage": "https://github.com/Gebec/symfony-landing-page#readme",
    "browserslist": "> 0.5%, last 2 versions, not dead",
    "devDependencies": {
        "@parcel/transformer-sass": "^2.7.0",
        "parcel": "^2.7.0"
    }
}
